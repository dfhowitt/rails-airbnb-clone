<%= simple_form_for @property do |f| %>
<%= f.input :name %>
<%= f.input :property_type %>
<%= f.input :location %>
<%= f.input :description %>
<%= f.input :price %>
<%= f.input :capacity %>
<%= f.input :availability %>
<%= f.input :photos, as: :file, input_html: { multiple: true } %>

<%= f.submit 'Register your luxurious property!'%>
<% end %>
